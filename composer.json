{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "andrewboy/search-box": "dev-master",
        "arrilot/laravel-widgets": "^3.11",
        "barryvdh/laravel-debugbar": "~2.4",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "bnjns/laravel-searchtools": "^1.3",
        "itsgoingd/clockwork": "^2.2",
        "jedrzej/searchable": "^0.0.14",
        "laracasts/flash": "^3.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "~2.1.0",
        "overtrue/laravel-uploader": "^0.0.8",
        "phpoffice/phpspreadsheet": "^1.3",
        "san4io/eloquent-filter": "^0.2.0"
    },
    "require-dev": {
        "crestapps/laravel-code-generator": "^2.2",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "filp/whoops": "~2.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "extra":{
        "laravel":{
            "dont-discover":[

            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
